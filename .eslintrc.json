{
    "env": {
        "browser": true,
        "es2021": true,
        "jest":true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended",
        "plugin:jest/recommended"
      ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "jest",
        "react"   
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "react/prop-types": "off",
        "no-shadow":"off"
    }
}
